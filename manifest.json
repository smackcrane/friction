{
  "manifest_version": 3,
  "name": "Friction",
  "version": "0.1",

  "description": "Remove some frictionless features of modern web apps.",

  "icons": {
    "48": "icons/error-med-48.png"
  },

  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*",
        "*://*.reddit.com/*",
        "*://*.linkedin.com/*",
        "*://*.stackexchange.com/*",
        "*://*.stackoverflow.com/*"
      ],
      "js": ["feedEater.js"]
    }
  ]
}
